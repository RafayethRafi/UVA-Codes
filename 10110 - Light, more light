
//BISMILLAHIR RAHMANIR RAHIM

#include<bits/stdc++.h>
using namespace std;
#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define  endl '\n'
int main()
{
    optimize();

    long long n;
    while(cin>>n && n)
    {
        long long x = sqrt(n);
        if(x*x==n) cout<<"yes"<<endl;
        else
            cout<<"no"<<endl;

    }

    return 0;
}

